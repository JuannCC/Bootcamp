package bootcamp;
/**
 * @author juancruzcrotta
 */
public class Bootcamp {

    public static void main(String[] args) {
    int d=suma(10,20,30);
    System.out.println(d);
    
    Bootcamp micoche = new Bootcamp();
    int e=micoche.coche(2);
    System.out.println(e);

    
}
public static int suma(int a, int b , int c){
    return a+b+c;
}
public int coche(int puertas){
     return ++puertas;
}
}
